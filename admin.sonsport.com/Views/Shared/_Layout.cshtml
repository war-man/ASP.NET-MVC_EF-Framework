<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Admin - Trang chủ Admin</title>

    <!-- Bootstrap core CSS-->
    <link href="~/Assets/TemplateAdmin/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom fonts for this template-->
    <link href="~/Assets/TemplateAdmin/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">

    <!-- Page level plugin CSS-->
    <link href="~/Assets/TemplateAdmin/vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="~/Assets/TemplateAdmin/css/sb-admin.css" rel="stylesheet">

</head>

<body id="page-top">

    @RenderPage("~/Views/Shared/_Header.cshtml")

    <div id="wrapper">

        @RenderPage("~/Views/Shared/_Sidebar.cshtml")

        <div id="content-wrapper">

            <div class="container-fluid">
                @RenderBody()
            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- /.content-wrapper -->
    </div>

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    @Html.Partial("_LogoutModal")

    <!-- Bootstrap core JavaScript-->
    <script src="~/Assets/TemplateAdmin/vendor/jquery/jquery.min.js"></script>
    <script src="~/Assets/TemplateAdmin/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/Assets/TemplateAdmin/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Page level plugin JavaScript-->
    <script src="~/Assets/TemplateAdmin/vendor/chart.js/Chart.min.js"></script>
    <script src="~/Assets/TemplateAdmin/vendor/datatables/jquery.dataTables.js"></script>
    <script src="~/Assets/TemplateAdmin/vendor/datatables/dataTables.bootstrap4.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="~/Assets/TemplateAdmin/js/sb-admin.min.js"></script>

    <!-- Demo scripts for this page-->
    <script src="~/Assets/TemplateAdmin/js/demo/datatables-demo.js"></script>
    <script src="~/Assets/TemplateAdmin/js/demo/chart-area-demo.js"></script>

    <script src="~/Assets/Scripts/bootbox.min.js"></script>
    <script src="~/Assets/SonSiHoJS/ssh.js"></script>
    @RenderSection("scripts", false)
</body>

</html>
